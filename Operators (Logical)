public class Logical 
{
    public static void main(String[] args) {
        int a = 8;
        int b = 5;

        // Logical AND (&&)
        System.out.println("a > 5 && b < 10 : " + (a > 5 && b < 10));

        // Logical OR (||)
        System.out.println("a > 10 || b < 10 : " + (a > 10 || b < 10));

        // Logical NOT (!)
        System.out.println("!(a < b) : " + !(a < b));
    }
}
